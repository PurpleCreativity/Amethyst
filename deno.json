{
  "name": "@purple-creativity/amethyst",
  "version": "2.0.0",
  "exports": {
    ".": "./src/main.ts"
  },
  "tasks": {
    "install": "deno install --allow-scripts=npm:@biomejs/biome@1.9.4",
    "dev": "deno run -A --watch ./src/main.ts --dev",
    "prod": "deno run -A ./src/main.ts",

    "lint": "deno run -A npm:@biomejs/biome lint .",
    "lint:fix": "deno run -A npm:@biomejs/biome lint --write .",

    "format": "deno run -A npm:@biomejs/biome format .",
    "format:fix": "deno run -A npm:@biomejs/biome format --write .",

    "organized-imports": "deno run -A npm:@biomejs/biome check --formatter-enabled=false --linter-enabled=false --organize-imports-enabled=true .",
    "organized-imports:fix": "deno run -A npm:@biomejs/biome check --formatter-enabled=false --linter-enabled=false --organize-imports-enabled=true --write .",

    "check": "deno run -A npm:@biomejs/biome check .",
    "check:fix": "deno run -A npm:@biomejs/biome check --write ."
  },
  "nodeModulesDir": "auto",
  "license": "MIT",
  "compilerOptions": {
    "types": ["./src/types/main.d.ts"]
  },
  "imports": {
    "@biomejs/biome": "npm:@biomejs/biome@^1.9.4",
    "@std/assert": "jsr:@std/assert@1",
    "axios": "npm:axios@^1.7.7",
    "chalk": "npm:chalk@^5.3.0",
    "discord-api-types": "npm:discord-api-types@^0.37.103",
    "discord.js": "npm:discord.js@^14.16.3",
    "dotenv": "npm:dotenv@^16.4.5",
    "express": "npm:express@^4.21.1",
    "express-rate-limit": "npm:express-rate-limit@^7.4.1",
    "mongoose": "npm:mongoose@^8.8.0",
    "wrapblox": "npm:wrapblox@^0.5.8"
  }
}
